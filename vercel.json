{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": ["install_ffmpeg.sh"]
            }
        }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "api/index.py" }
    ]
}
